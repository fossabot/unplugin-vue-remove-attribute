{
  "name": "unplugin-vue-remove-attribute",
  "version": "1.0.1",
  "description": "Remove attributes from vue templates at build time ",
  "author": "Marek VospÄ›l <marek@vospel.cz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marekvospel/unplugin-vue-remove-attribute"
  },
  "homepage": "https://github.com/marekvospel/unplugin-vue-remove-attribute#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint ."
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs",
      "types": "./dist/vite.d.ts"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs",
      "types": "./dist/rollup.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.37",
    "unplugin": "^0.9.3"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": ">=3"
  },
  "devDependencies": {
    "@vospel/eslint-config": "^1.4.9",
    "eslint": "^8.22.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "packageManager": "pnpm@7.9.3"
}
